{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": ["./app/types", "./node_modules/@types"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@/public/*":["./public/*"],
      "@/ela-api/*":["./app/api/*"],
      "@/ela-components":["./app/components/ela-components"],
      "@/ela-form":["./app/components/form/ela-form"],
      "@/types":["./app/types/index.ts"],
      "@/ela-hooks":["./app/hooks/ela-hooks"],
      "@/global-styles":["./app/variables.sass"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/Frame.tsx", "app/globals.js", "app/(views)/ingreso/login/page.tsx", "app/test2/useInput.js", "middleware.ts", "app/layout.tsx", "app/components/form/Form.tsx"],
  "exclude": ["node_modules"]
}
